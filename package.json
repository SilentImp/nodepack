{
  "name": "backendtest",
  "version": "1.0.0",
  "description": "just test",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build public",
    "start": "export NODE_ENV=local && webpack-dev-server --hot --inline",
    "build": "npm run clean && export NODE_ENV=production && npm-run-all build:production:*",
    "build:production:client": "webpack -p --config ./webpack.config.js --env=production",
    "build:production:server": "webpack -p --config ./webpack.server.config.js --env=production",
    "build:dev": "npm run clean && export NODE_ENV=dev && npm-run-all build:dev:*",
    "build:dev:client": "webpack --config ./webpack.config.js --env=dev",
    "build:dev:server": "webpack --config ./webpack.server.config.js --env=dev",
    "build:local": "npm run clean && export NODE_ENV=local && npm-run-all build:local:*",
    "build:local:client": "webpack --config ./webpack.config.js --env=local --progress --debug --colors --display-reasons --verbose --display-chunks",
    "build:local:server": "webpack --config ./webpack.server.config.js --env=local --progress --debug --colors --display-reasons --verbose --display-chunks",
    "build:doc": "documentation build src/*.js* -o public -f html",
    "test": "export NODE_ENV=test && jest --no-cache",
    "test:watch": "export NODE_ENV=test && jest --no-cache --watch",
    "test:coverage": "export NODE_ENV=test && jest --no-cache --coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint ./src/** --ext .jsx,.js --color -f stylish --fix",
    "lint:css": "stylelint './src/**/*.pcss' --color -r -f verbose --fix",
    "precommit": "git add . -A && lint-staged",
    "commitmsg": "commit-msg ${GIT_PARAMS}"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --color -f stylish --fix",
      "git add"
    ],
    "*.pcss": [
      "stylelint --color -r -f verbose --fix",
      "git add"
    ]
  },
  "author": "Anton Nemtsev <thesilentimp@gmail.com>",
  "license": "ISC",
